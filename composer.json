{
	"name": "binarystash/pdf-watermarker",
	"license": "MIT",
	"description": "Watermark or stamp PDF documents",
	"authors": [
        {
            "name": "BinaryStash"
        }
    ],
	"require": {
		"setasign/fpdf": "dev-master",
		"setasign/fpdi": "dev-master",
      	"ext-exif": "*",
		"ext-gd": "*"
    },
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"evert/phpdoc-md" : "~0.0.7"
	},
	"autoload": {
		"psr-4": {
			"Watermarker\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"minimum-stability": "stable"
}
